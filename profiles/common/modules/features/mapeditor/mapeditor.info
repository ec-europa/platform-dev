name = Mapeditor
description = Provides map entity that allows users to create maps using the corporate webtools JavaScript library.
core = 7.x
dependencies[] = entity
dependencies[] = mapeditor_layer
dependencies[] = views
dependencies[] = inline_entity_form
dependencies[] = entityreference
files[] = mapeditor.controller.inc
project path = profiles/common/modules/features/mapeditor
multisite_version = 2.1
package = Maps
php = 5.2.4

; Feature set entry for the Country layer
feature_sets[mapeditor_country][title] = 'Country layer'
feature_sets[mapeditor_country][description] = 'Allows users to display country outlines on a map'
feature_sets[mapeditor_country][documentation] = 'https://webgate.ec.europa.eu/fpfis/wikis/x/iKNDBg'
feature_sets[mapeditor_country][category] = 'Maps'
feature_sets[mapeditor_country][icon] = ''
feature_sets[mapeditor_country][font] = 'multisite-icon-location'
feature_sets[mapeditor_country][enable][] = mapeditor_country
feature_sets[mapeditor_country][disable][] = mapeditor_country

