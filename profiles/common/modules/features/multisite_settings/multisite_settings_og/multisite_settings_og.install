<?php

/**
 * @file
 * Code for the multisite_settings_og feature.
 */

/**
 * Implements hook_disable().
 */
function multisite_settings_og_disable() {
  drupal_set_message(t('Multisite Settings OG feature is now disable on your site.'));
}

/**
 * Implements hook_enable().
 */
function multisite_settings_og_enable() {
  drupal_set_message(t('Multisite Settings OG feature is now active on your site.'));
}
