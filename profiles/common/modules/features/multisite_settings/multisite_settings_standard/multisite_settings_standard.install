<?php
/**
 * @file
 * Code for the multisite_settings_standard feature.
 */

/**
 * Implements hook_enable().
 */
function multisite_settings_standard_enable() {
  drupal_set_message(t('Multisite Settings Standard feature is now active on your site.'));
}

/**
 * Implements hook_disable().
 */
function multisite_settings_standard_disable() {
  drupal_set_message(t('Multisite Settings Standard feature is now disable on your site.'));
}
