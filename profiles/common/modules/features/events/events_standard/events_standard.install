<?php

/**
 * @file
 * Install the feature events.
 */

/**
 * Implements hook_enable().
 */
function events_standard_enable() {
  drupal_set_message(t('events_standard feature is now active on your site.'));
}

/**
 * Implements hook_disable().
 */
function events_standard_disable() {
  // Example.
  drupal_set_message(t('events_standard feature is now inactive on your site.'));
}
